{
  "helpHtml.tpl": {
    "coverityAnalysisType": {
      "overview": [
        "Specify the default Coverity workflow you would like to run.",
        "Each workflow runs a set of Coverity commands with some default arguments. Additional command-specific arguments can be provided below.",
        "",
        "<strong>Full Analysis</strong> workflow commands:",
        "cov-build --dir ${WORKSPACE}/idir <em>Source Argument (build command)</em>",
        "cov-analyze --dir ${WORKSPACE}/idir",
        "cov-commit-defects --dir ${WORKSPACE}/idir --url ${COV_URL} --stream ${COV_STREAM}",
        "",
        "<strong>Incremental Analysis</strong> workflow commands:",
        "cov-build --dir ${WORKSPACE}/idir <em>Source Argument (build command)</em>",
        "cov-run-desktop --dir ${WORKSPACE}/idir --url ${COV_URL} --stream ${COV_STREAM} ${CHANGE_SET}",
        "cov-commit-defects --dir ${WORKSPACE}/idir --url ${COV_URL} --stream ${COV_STREAM}",
        "",
        "<strong>Buildless Capture (Project)</strong> workflow commands:",
        "cov-capture --project-dir <em>Source Argument (project directory)</em>",
        "cov-analyze --dir ${WORKSPACE}/idir",
        "cov-commit-defects --dir ${WORKSPACE}/idir --url ${COV_URL} --stream ${COV_STREAM}",
        "",
        "<strong>Buildless Capture (SCM)</strong> workflow commands:",
        "cov-capture --scm-url <em>Source Argument (scm url)</em>",
        "cov-analyze --dir ${WORKSPACE}/idir",
        "cov-commit-defects --dir ${WORKSPACE}/idir --url ${COV_URL} --stream ${COV_STREAM}"
      ]
    },
    "sourceArgument": {
      "overview": [
        "The argument that specifies the source for the given workflow.",
        "For <strong>Full Analysis</strong> and <strong>Incremental Analysis</strong>, this is the build command to pass to cov-build.",
        "For <strong>Buildless Capture (Project)</strong> this is the project directory.",
        "For <strong>Buildless Capture (SCM)</strong> this is the scm url."
      ]
    }
  }
}